fun isPrime of num is
    var ans <- True;
    var i<- 2;
    while i<num do
        if num%i==0 then
            ans <- False;
            i <- num;
        else
            i <- i + 1;
        endif;
    endwhile;
    if num<2 then
        ans <- False;
    endif;
    returns ans;
endfun;

var n <- 73;
var prime_check <- isPrime of n;
print prime_check;
# 0 means composite and 1 means prime#
